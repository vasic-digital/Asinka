# Maven Central Publishing
MAVEN_CENTRAL_USERNAME=your_sonatype_username
MAVEN_CENTRAL_PASSWORD=your_sonatype_password

# Signing Configuration
SIGNING_KEY_ID=your_key_id
SIGNING_PASSWORD=your_signing_password
SIGNING_SECRET_KEY_RING_FILE=/path/to/secring.gpg

# Library Version
VERSION_NAME=0.1.0
VERSION_CODE=1

# Optional: Demo App Configuration
DEMO_APP_SIGNING_KEY_ALIAS=your_alias
DEMO_APP_SIGNING_KEY_PASSWORD=your_key_password
DEMO_APP_SIGNING_STORE_PASSWORD=your_store_password